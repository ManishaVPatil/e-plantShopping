  <!-- Enable Replace WCToken Script -->
  <script type="text/javascript">
    function getCookie(name) {
      var nameEQ = name + "=";
      var ca = document.cookie.split(';');
      for (var i = 0; i < ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0) == ' ') c = c.substring(1, c.length);
          if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
      }
      return null;
    }
    var wcTokenForUser = getCookie("wcToken");
    var userId = wcTokenForUser && wcTokenForUser.substring(0, 9);
    var wcTokenSetInterval = (userId < 379611001) && setInterval(function() {
        if(wcTokenForUser) {
          if (getCookie("wcTrustedToken")) {
            var wcTrustedToken = getCookie("wcTrustedToken");
          }
          if (getCookie("roToken")) {
            var roToken = getCookie("roToken");
          }
          if (wcTokenForUser && userId && userId < 379611001 && !roToken) {
            console.log("User can be expired.Deleting the tokens--aem");
            document.cookie = "wcToken=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
            if (wcTrustedToken) {
                document.cookie = "wcTrustedToken=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
            }
            if(wcTrustedToken && wcTokenForUser){
              clearInterval(wcTokenSetInterval);
            }
          }
          else{
            console.log("User cannot be expired--aem");
          }
        }
      }, 500);
  </script>
  <!-- End Enable Replace WCToken Script -->

	
	
